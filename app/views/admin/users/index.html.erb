<div>
    <ul>
        <%= "Users" %>
                <% users = User.all %>
                <% users.each do |user| %>
                <li>
                    <%= user.email %>
                    <%= user.role %>
                    <%= link_to 'Edit', edit_admin_user_path(user) %>
                    <%= button_to "Delete", admin_user_path(user), method: :delete, data: {confirm: 'This process is irreversible. Continue?' } %>
                </li>
             <% end %>
    </ul>
</div>